import pandas as pd
from scipy.stats import ttest_ind


df=pd.read_csv("lifestyle.csv")


group1=df[df["lifecycle"]=="first_6mnt"]["revenue"]
group2=df[df["lifecycle"]=='above_6']["revenue"]





tstat,p_val=ttest_ind(group1,group2,  equal_var=True)


print(tstat)
print(p_val)
print(group1.mean())
print(group2.mean())
print(group1.std())
print(group2.std())






alpha=0.005
if(p_val<=alpha):
    print("null hypothesis rejected")
else:
    print("failed to acccept the null hypothesis")
